    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.6",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "cross-env": "^10.0.0",
