{
  "name": "happy-tts-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass",
    "*.less"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:simple": "tsc && vite build --mode simple",
    "build:minimal": "tsc && vite build --mode minimal",
    "build:analyze": "tsc && vite build --mode analyze",
    "build:optimized": "tsc && vite build --mode production",
    "preview": "vite preview",
    "serve": "node static-server.js",
    "serve:prod": "npm run build && node static-server.js",
    "test:rate-limit": "node test-rate-limit.js",
    "security-check": "node security-check.js",
    "test": "vitest",
    "analyze": "npm run build:analyze && npx vite-bundle-analyzer dist",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "fix:rollup": "node fix-rollup-deps.js",
    "fix:rollup:win": "node fix-rollup-win.js",
    "fix:rollup:smart": "chmod +x fix-rollup-deps.sh && ./fix-rollup-deps.sh"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^4.2.0",
    "@heroicons/react": "^2.2.0",
    "@microsoft/clarity": "^1.0.0",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@react-email/components": "0.5.1",
    "@simplewebauthn/browser": "^13.1.2",
    "@simplewebauthn/server": "^13.1.2",
    "@simplewebauthn/types": "^12.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/validator": "^13.15.2",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "dompurify": "^3.2.6",
    "express-rate-limit": "^7.5.0",
    "form-data": "^4.0.4",
    "framer-motion": "^12.23.12",
    "happy-tts-frontend": "file:",
    "html-docx-js": "^0.3.1",
    "html2canvas": "^1.4.1",
    "idb": "^8.0.3",
    "javascript-obfuscator": "^4.0.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.2",
    "jszip": "^3.10.1",
    "katex": "^0.16.22",
    "lodash.merge": "^4.6.2",
    "lucide-react": "^0.540.0",
    "marked": "^16.2.0",
    "marked-katex-extension": "^5.1.5",
    "md5": "^2.3.0",
    "mermaid": "^11.10.1",
    "prismjs": "^1.30.0",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.8.2",
    "react-syntax-highlighter": "^15.6.1",
    "react-toastify": "^11.0.5",
    "rollup": "^4.48.1",
    "swagger-ui-react": "^5.27.0",
    "tailwind-merge": "^3.3.1",
    "validator": "^13.15.15",
    "vite-plugin-auto-i18n": "^0.0.9"
